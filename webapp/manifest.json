{
	"_version": "1.7.0",
	"sap.app": {
		"id": "com.sanketMyFirstUI5App",
		"type": "application",
		"i18n": "i18n/i18n.properties",
		"applicationVersion": {
			"version": "1.0.0"
		},
		"title": "{{appTitle}}",
		"description": "{{appDescription}}",
		"sourceTemplate": {
			"id": "ui5template.basicSAPUI5ApplicationProject",
			"version": "1.40.12"
		}
	},

	"sap.ui": {
		"technology": "UI5",
		"icons": {
			"icon": "",
			"favIcon": "",
			"phone": "",
			"phone@2": "",
			"tablet": "",
			"tablet@2": ""
		},
		"deviceTypes": {
			"desktop": true,
			"tablet": true,
			"phone": true
		},
		"supportedThemes": [
			"sap_hcb",
			"sap_belize"

		]
	},

	"sap.ui5": {
		"rootView": {
			"viewName": "com.sanketMyFirstUI5App.view.View1",
			"type": "XML"
		},
		"dependencies": {
			"minUI5Version": "1.30.0",
			"libs": {
				"sap.ui.core": {},
				"sap.m": {},
				"sap.ui.layout": {},
				"sap.ushell": {},
				"sap.collaboration": {},
				"sap.ui.comp": {},
				"sap.uxap": {}
			}
		},
		"contentDensities": {
			"compact": true,
			"cozy": true
		},
		"models": {
			"i18n": {
				"type": "sap.ui.model.resource.ResourceModel",
				"settings": {
					"bundleName": "com.sanketMyFirstUI5App.i18n.i18n"
				}
			},
			"oGlobalModel": {
				"type": "sap.ui.model.json.JSONModel",
				"settings": {
					"defaultBindingMode": "TwoWay"
				}
			}

		},
		"resources": {
			"css": [{
				"uri": "css/style.css"
			}]
		},
		"routing": {
			"config": {
				"routerClass": "sap.m.routing.Router",
				"viewType": "XML",
				"async": true,
				"viewPath": "com.sanketMyFirstUI5App.view",
				"controlAggregation": "pages",
				"controlId": "app"
			},
			"routes": [{
				"name": "RoutetoView1",
				"pattern": "",
				"target": [
					"TargetView1"
				]
			}, {
				"name": "RoutetoView2",
				"pattern": "View2",

				"target": [
					"TargetView2"
				]
			}],
			"targets": {
				"TargetView1": {
					"transition": "slide",
					"viewType": "XML",
					"viewLevel": 1,
					"controlAggregation": "pages",
					"viewId": "View1",
					"viewName": "View1"
				},
				"TargetView2": {
					"controlAggregation": "pages",
					"viewName": "View2",
					"viewId": "View2",
					"viewLevel": 2,
					"transition": "slide"
				}

			}
		}
	}
}